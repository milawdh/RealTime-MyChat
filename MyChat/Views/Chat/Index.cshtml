            <div class="uk-modal-body">

                <div class="d-grid">
                    <div class="d-flex justify-content-center">
                        <img src="~/Resources/NoPofile.jpg" class="uk-border-circle" width="90" height="90" alt="">
                    </div>
                    <h4 class="uk-text-center m-0">|Milad|</h4>
                </div>

                <form action="" class="d-grid">
                    <label class="uk-form-label">Name</label>
                    <div class="uk-form-controls">
                        <input class="uk-input" type="text">
                    </div>
                    <label class="uk-form-label">Bio</label>
                    <div class="uk-form-controls">
                        <textarea class="uk-textarea" type="text"></textarea>
                    </div>
                    <label class="uk-form-label">UserName</label>
                    <div class="uk-form-controls">
                        <input class="uk-input" type="text">
                    </div>
                    <label class="uk-form-label">Tell</label>
                    <div class="uk-form-controls">
                        <input class="uk-input uk-disabled" value="09214121204" type="text">
                    </div>
                </form>
                <form action=""></form>
                <p class="uk-text-left">
                    <button class="uk-button uk-button-default uk-modal-close uk-border-rounded"
                            type="button">
                        Cancel
                    </button>
                    <button class="uk-button uk-button-primary uk-border-rounded" type="button">Save</button>
                </p>
            </div>
        </div>
    </div>


    <div id="ProfileModal" uk-modal>
        <div class="uk-modal-dialog rounded-2 uk-border-rounded">
            <div class="uk-modal-header uk-border-rounded _nav d-flex justify-content-between"
                 style="background-color: #009688;">

                <span class="uk-modal-title h2">Info</span>
